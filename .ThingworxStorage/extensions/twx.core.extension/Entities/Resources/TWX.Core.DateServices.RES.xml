<Entities>
	<Resources>
		<Resource
			name="DateServices"
			className="twx.core.date.DateServices"
			description="Ressource providing basic Date Operations with TimeZone"
			projectName="TWX.Core"
			tags="TWX.Core:Service"
			aspect.isEditableExtensionObject="false"
			aspect.isCreatable="false">
			<RunTimePermissions>
				<Permissions
					resourceName="*">
					<PropertyRead>
						<Principal
							isPermitted="true"
							name="TWX.Core.Default_UG"
							type="Group"></Principal>
					</PropertyRead>
					<PropertyWrite>
						<Principal
							isPermitted="true"
							name="TWX.Core.Default_UG"
							type="Group"></Principal>
					</PropertyWrite>
					<ServiceInvoke>
						<Principal
							isPermitted="true"
							name="TWX.Core.Default_UG"
							type="Group"></Principal>
					</ServiceInvoke>
					<EventInvoke>
						<Principal
							isPermitted="true"
							name="TWX.Core.Default_UG"
							type="Group"></Principal>
					</EventInvoke>
					<EventSubscribe>
						<Principal
							isPermitted="true"
							name="TWX.Core.Default_UG"
							type="Group"></Principal>
					</EventSubscribe>
				</Permissions>
			</RunTimePermissions>
			<VisibilityPermissions>
				<Visibility>
					<Principal
						isPermitted="true"
						name="TWX.Core.Default_OR"
						type="Organization"></Principal>
				</Visibility>
			</VisibilityPermissions>
		</Resource>

	</Resources>
</Entities>